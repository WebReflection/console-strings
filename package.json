{
  "name": "console-strings",
  "version": "0.2.7",
  "main": "index.cjs",
  "module": "index.js",
  "type": "module",
  "scripts": {
    "build": "ascjs index.js index.cjs && ascjs browser.js browser.cjs && npm run test",
    "test": "c8 node ./test/index.js && node ./test/index.cjs",
    "coverage": "mkdir -p ./coverage; c8 report --reporter=text-lcov > ./coverage/lcov.info"
  },
  "keywords": [
    "devtools",
    "log",
    "style",
    "escape",
    "terminal"
  ],
  "files": [
    "index.js",
    "index.cjs",
    "browser.js",
    "browser.cjs",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./index.js",
      "default": "./index.cjs"
    },
    "./browser": {
      "import": "./browser.js",
      "default": "./browser.cjs"
    },
    "./package.json": "./package.json"
  },
  "author": "Andrea Giammarchi",
  "license": "MIT",
  "description": "String utilities that understand terminal escape sequences",
  "devDependencies": {
    "ascjs": "^6.0.3",
    "c8": "^10.1.2"
  }
}
